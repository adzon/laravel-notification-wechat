{
    "name": "adzon/laravel-notification-wechat",
    "description": "Use Wechat to send notification.",
    "homepage": "https://github.com/adzon/laravel-notification-wechat",
    "license": "MIT",
    "authors": [
        {
            "name": "adzon",
            "email": "76762@qq.com",
            "homepage": "http://www.huleping.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.4.*",
        "laravel/tinker": "~1.0",
        "overtrue/laravel-wechat": "~3.0",
        "overtrue/wechat": "~3.1"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\Wechat\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\Wechat\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
