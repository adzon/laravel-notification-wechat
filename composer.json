{
    "name": "adzon/laravel-notification-wechat",
    "description": "Use Wechat to send notification.",
    "homepage": "https://github.com/adzon/laravel-notification-wechat",
    "license": "MIT",
    "authors": [
        {
            "name": "adzon",
            "email": "76762@qq.com",
            "homepage": "http://www.huleping.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/notifications": "^5.3",
        "illuminate/support": "^5.1|^5.2|^5.3"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\Wechat\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\Wechat\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
